pipeline {
    agent any

    stages {
        stage('get project from github') {
            steps {
               sh 'gh repo clone Chiheb97/Jenkins-pipeline-spring'
            }
        }
        stage('build') {
            steps {
                sh 'mvn install'
            }
        }
        stage('test mvn') {
            steps {
                sh 'mvn clean test'
            }
        }
    }
}